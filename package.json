  "scripts": {
    "start": "node server.js",
    "test": "karma start --single-run",
    "test-coverage": "karma start karma.conf.coverage.js",
    "clean-build": "rm -rf dist",
    "build": "npm run clean-build && webpack -p --config webpack.production.config.js --progress --color --profile && npm run build-index",
    "build-index": "rm -f dist/index.html && cp index_dist.html dist/index.html",
    "bundle-stats": "webpack -p --config webpack.production.config.js --progress --json > bundle-stats.json && analyze-bundle-size bundle-stats.json"
  }
